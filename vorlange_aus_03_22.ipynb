   "source": [
    "import random\n",
    "\n",
    "aerzte={}\n",
    "monat={}\n",
    "alle_versionen = []\n",
    "\n",
    "def arzt(name,klinik,nur12=False,nurhaus=False,maximum=99, wunsch=[], frei=[]):\n",
    "    aerzte[name]={\n",
    "        \"klinik\":klinik,\n",
    "        \"wunsch\":wunsch,\n",
    "        \"frei\":frei,\n",
    "        \"nur12\":nur12,\n",
    "        \"nurhaus\":nurhaus,\n",
    "        \"maximum\":maximum,\n",
    "        \"dienste\":[],\n",
    "        \"dienstanzahl\":0,\n",
    "        \"punkte\":0,\n",
    "        \"wochenenden\":0\n",
    "    }\n",
    "\n",
    "\n",
    "def set_monat(tage,ersterwochentag,feiertage=[]):\n",
    "    for t in range(1,tage+1):\n",
    "        monat[t]={\n",
    "            \"wochentag\":False,\n",
    "            \"punkte\":1,\n",
    "            \"imc\":0,\n",
    "            \"rth\":0,\n",
    "            \"na\":0,\n",
    "            \"nfa\":[],\n",
    "            \"haus\":[],\n",
    "            \"kliniken\":{},\n",
    "            \"frei\":False\n",
    "        }\n",
    "    for t in range(1,tage+1):\n",
    "        if t == 1:\n",
    "            monat[t][\"wochentag\"] = ersterwochentag\n",
    "        else:\n",
    "            if monat[t-1][\"wochentag\"] == \"montag\":\n",
    "                monat[t][\"wochentag\"] = \"dienstag\"\n",
    "            elif monat[t-1][\"wochentag\"] == \"dienstag\":\n",
    "                monat[t][\"wochentag\"] = \"mittwoch\"\n",
    "            elif monat[t-1][\"wochentag\"] == \"mittwoch\":\n",
    "                monat[t][\"wochentag\"] = \"donnerstag\"\n",
    "            elif monat[t-1][\"wochentag\"] == \"donnerstag\":\n",
    "                monat[t][\"wochentag\"] = \"freitag\"\n",
    "            elif monat[t-1][\"wochentag\"] == \"freitag\":\n",
    "                monat[t][\"wochentag\"] = \"samstag\"\n",
    "            elif monat[t-1][\"wochentag\"] == \"samstag\":\n",
    "                monat[t][\"wochentag\"] = \"sonntag\"\n",
    "            elif monat[t-1][\"wochentag\"] == \"sonntag\":\n",
    "                monat[t][\"wochentag\"] = \"montag\"\n",
    "                \n",
    "        if monat[t][\"wochentag\"] == \"freitag\" or monat[t][\"wochentag\"] == \"sonntag\":\n",
    "            monat[t][\"punkte\"] = 2\n",
    "        if monat[t][\"wochentag\"] == \"samstag\":\n",
    "            monat[t][\"punkte\"] = 3\n",
    "        \n",
    "        if monat[t][\"wochentag\"] == \"sonntag\" or monat[t][\"wochentag\"] == \"samstag\":\n",
    "            monat[t][\"frei\"] = True\n",
    "        \n",
    "    for t in feiertage:\n",
    "        monat[t][\"frei\"] == True\n",
    "        if t < tage+1:\n",
    "            monat[t][\"punkte\"] = 2\n",
    "        if t > 1:\n",
    "            monat[t-1][\"punkte\"] = monat[t-1].get(\"punkte\",0) + 1\n",
    "\n",
    "\n",
    "def set_imc(*imc): #IMC-Dienst-Kliniken als Liste eintragen\n",
    "    for i in range(len(imc)):\n",
    "        monat[i+1][\"imc\"]=imc[i]\n",
    "\n",
    "\n",
    "def set_rth(*rth): #RTH-Dienste als liste von tuple (datum,klinik) eintragen\n",
    "    for i in rth:\n",
    "        monat[i[0]][\"rth\"] = i[1]\n",
    "\n",
    "\n",
    "def set_na(*na): #NA-Dienste als liste von tuple (datum,klinik) eintragen\n",
    "    for i in na:\n",
    "        monat[i[0]][\"na\"] = i[1]\n",
    "\n",
    "\n",
    "def get_kliniken(): #Zählt die Kliniken der IMC-, NA-, und RTH-Dienste\n",
    "    for key in monat:\n",
    "        tag = monat[key]\n",
    "        imc = tag[\"imc\"]\n",
    "        na = tag[\"na\"]\n",
    "        tag[\"kliniken\"][imc] = tag.get(\"kliniken\").get(imc,0) + 1\n",
    "        tag[\"kliniken\"][na] = tag.get(\"kliniken\").get(na,0) + 1\n",
    "        if key > 1:\n",
    "            tag = monat[key-1]\n",
    "            rth = monat[key][\"rth\"]\n",
    "            tag[\"kliniken\"][rth] = tag.get(rth, 0) + 1\n",
    "\n",
    "\n",
    "def wunsch(name, w=[], f=[]):\n",
    "    aerzte[name][\"wunsch\"] = w\n",
    "    aerzte[name][\"frei\"] = f\n",
    "\n",
    "\n",
    "def get_max_we_und_feiertage(): #zählt alle Dienste mit > 1 Punkt zusammen und teilt sie durch die AZ der Ärzte\n",
    "    counter = 0\n",
    "    for tag in monat:\n",
    "        if monat[tag][\"punkte\"] > 1:\n",
    "            counter += 1\n",
    "    counter *= 2\n",
    "    maximum = int(counter / len(aerzte)+0.5)+1\n",
    "    return maximum\n",
    "\n",
    "\n",
    "def nfa_bei_nurhaus(name, reihe):\n",
    "    if reihe == \"nfa\" and aerzte[name][\"nurhaus\"] == True:\n",
    "        return True\n",
    "    \n",
    "\n",
    "def maximum_erreicht(name):\n",
    "    if (aerzte[name][\"dienstanzahl\"] == aerzte[name][\"maximum\"] or \n",
    "        aerzte[name][\"dienstanzahl\"] > aerzte[name][\"maximum\"]):\n",
    "        return True\n",
    "\n",
    "    \n",
    "def dienste_in_naher_beziehung(name, datum):\n",
    "    if not aerzte[name][\"nur12\"]:\n",
    "        schutzzone = [datum-2, datum-1, datum, datum+1, datum+2]\n",
    "        if monat[datum][\"wochentag\"] == \"sonntag\":\n",
    "            schutzzone.append(datum-3)\n",
    "        if monat[datum][\"wochentag\"] == \"donnerstag\":\n",
    "            schutzzone.append(datum+3)\n",
    "        for d in schutzzone:\n",
    "            if d in aerzte[name][\"dienste\"]:\n",
    "                return True\n",
    "\n",
    "        \n",
    "def kliniken_voll(name, datum):\n",
    "    klinik = aerzte[name].get(\"klinik\")\n",
    "    klinikanzahl_an_datum = monat[datum][\"kliniken\"].get(klinik,0)\n",
    "    if (monat[datum][\"punkte\"] == 1 or\n",
    "        (monat[datum][\"punkte\"] == 2 and monat[datum][\"wochentag\"] == \"sonntag\")):\n",
    "        if klinikanzahl_an_datum > 1:\n",
    "            return True\n",
    "\n",
    "\n",
    "def wochentag_aber_nur12(name, datum):\n",
    "    if aerzte[name].get(\"nur12\"):\n",
    "        if monat[datum].get(\"punkte\") == 1:\n",
    "            return True\n",
    "        if monat[datum].get(\"punkte\") == 3:\n",
    "            return False\n",
    "        if datum > 1:\n",
    "            if monat[datum-1].get(\"punkte\") == 1:\n",
    "                return True\n",
    "\n",
    "            \n",
    "def dienst_schon_belegt(datum, reihe):\n",
    "    belegung = monat[datum].get(reihe)\n",
    "    if not (isinstance(belegung, list) and len(belegung) < 2):\n",
    "        return True\n",
    "\n",
    "    \n",
    "def wunschfrei(name, datum):\n",
    "    if datum in aerzte[name].get(\"frei\"):\n",
    "        return True\n",
    "            \n",
    "        \n",
    "def vergabe_pruefen(name, datum, reihe):\n",
    "        \n",
    "    if (\n",
    "        nfa_bei_nurhaus(name, reihe) or\n",
    "        maximum_erreicht(name) or\n",
    "        dienste_in_naher_beziehung(name, datum) or\n",
    "        kliniken_voll(name, datum) or\n",
    "        wochentag_aber_nur12(name, datum) or\n",
    "        dienst_schon_belegt(datum, reihe) or\n",
    "        wunschfrei(name, datum)\n",
    "    ):\n",
    "        return False\n",
    "    return True\n",
    "  \n",
    "\n",
    "def dienst_vergeben(name, datum, reihe):\n",
    "    tag = monat[datum]\n",
    "    arzt = aerzte[name]\n",
    "    if arzt[\"nur12\"] or len(tag[reihe]) == 1:\n",
    "        tag[reihe].append(name)\n",
    "        arzt[\"punkte\"] += tag[\"punkte\"]/2\n",
    "        arzt[\"dienstanzahl\"] += 0.5\n",
    "    else:\n",
    "        tag[reihe] = name\n",
    "        arzt[\"punkte\"] += tag[\"punkte\"]\n",
    "        arzt[\"dienstanzahl\"] += 1\n",
    "    arzt[\"dienste\"].append(datum)\n",
    "    tag[\"kliniken\"][arzt[\"klinik\"]] = tag[\"kliniken\"].get(arzt[\"klinik\"], 0) + 1\n",
    "    if tag[\"punkte\"] > 1:\n",
    "        arzt[\"wochenenden\"] += 1\n",
    "    \n",
    "    \n",
    "def wuensche_verteilen():\n",
    "    wuensche = []\n",
    "    \n",
    "    aerzte_nach_prio = []\n",
    "    for arzt in aerzte:\n",
    "        if aerzte[arzt][\"nur12\"]:\n",
    "            aerzte_nach_prio.append(arzt)\n",
    "    for arzt in aerzte:\n",
    "        if aerzte[arzt][\"nurhaus\"] and not aerzte[arzt][\"nur12\"]:\n",
    "            aerzte_nach_prio.append(arzt)\n",
    "    for arzt in aerzte:\n",
    "        if not aerzte[arzt][\"nur12\"] and not aerzte[arzt][\"nurhaus\"]:\n",
    "            aerzte_nach_prio.append(arzt)\n",
    "    \n",
    "    for arzt in aerzte_nach_prio:\n",
    "        for wunsch in aerzte[arzt][\"wunsch\"]:\n",
    "            wuensche.append((arzt, wunsch))\n",
    "    \n",
    "    for wunsch in wuensche:\n",
    "        name = wunsch[0]\n",
    "        datum = wunsch[1]\n",
    "        if vergabe_pruefen(name, datum, \"nfa\"):\n",
    "            dienst_vergeben(name, datum, \"nfa\")\n",
    "        elif vergabe_pruefen(name, datum, \"haus\"):\n",
    "            dienst_vergeben(name, datum, \"haus\")\n",
    "\n",
    "\n",
    "def prio_arzt_we_daz_pkt():\n",
    "    prio = []\n",
    "    for arzt in aerzte:\n",
    "        wochenenden = aerzte[arzt].get(\"wochenenden\")\n",
    "        dienste = aerzte[arzt].get(\"dienstanzahl\")\n",
    "        punkte = aerzte[arzt].get(\"punkte\")\n",
    "        prio.append((arzt, wochenenden, dienste, punkte))\n",
    "    prio = sorted(prio, key = lambda x: (x[1], x[2], x[3]))\n",
    "    arzt_prio = []\n",
    "    for arzt in prio:\n",
    "        arzt_prio.append(arzt[0])\n",
    "    arzt_prio.reverse()\n",
    "    return arzt_prio\n",
    "\n",
    "\n",
    "def prio_arzt_daz_pkt():\n",
    "    prio = []\n",
    "    for arzt in aerzte:\n",
    "        dienste = aerzte[arzt].get(\"dienstanzahl\")\n",
    "        punkte = aerzte[arzt].get(\"punkte\")\n",
    "        prio.append((arzt, dienste, punkte))\n",
    "    prio = sorted(prio, key = lambda x: (x[1], x[2]))\n",
    "    arzt_prio = []\n",
    "    for arzt in prio:\n",
    "        arzt_prio.append(arzt[0])\n",
    "    arzt_prio.reverse()\n",
    "    return arzt_prio\n",
    "\n",
    "\n",
    "def liste_we_offen(reihe):\n",
    "    l = []\n",
    "    for tag in monat:\n",
    "        besetzung = monat[tag].get(reihe)\n",
    "        if monat[tag].get(\"punkte\") > 1 and (isinstance(besetzung, list) and len(besetzung) < 2):\n",
    "            l.append(tag)\n",
    "    return l\n",
    "\n",
    "\n",
    "def liste_rest_offen(reihe):\n",
    "    l = []\n",
    "    for tag in monat:\n",
    "        besetzung = monat[tag].get(reihe)\n",
    "        if monat[tag].get(\"punkte\") == 1 and isinstance(besetzung, list):\n",
    "            l.append(tag)\n",
    "    return l\n",
    "\n",
    "\n",
    "def unbesetzt_eintragen(datum, reihe):\n",
    "    monat[datum][reihe] = \"unbesetzt\"\n",
    "\n",
    "def we_verteilen(reihe):\n",
    "    offen = liste_we_offen(reihe)\n",
    "    arzt_nach_prio = prio_arzt_we_daz_pkt()\n",
    "    while not len(offen) == 0:\n",
    "        datum = random.choice(offen)\n",
    "        vergabe_erfolgreich = False\n",
    "        while not len(arzt_nach_prio) == 0:\n",
    "            name = arzt_nach_prio.pop()\n",
    "            if vergabe_pruefen(name, datum, reihe):\n",
    "                dienst_vergeben(name, datum, reihe)\n",
    "                vergabe_erfolgreich = True\n",
    "                break\n",
    "        if not vergabe_erfolgreich:\n",
    "            unbesetzt_eintragen(datum, reihe)\n",
    "        offen = liste_we_offen(reihe)\n",
    "        arzt_nach_prio = prio_arzt_we_daz_pkt()\n",
    "\n",
    "\n",
    "def rest_verteilen(reihe):\n",
    "    offen = liste_rest_offen(reihe)\n",
    "    arzt_nach_prio = prio_arzt_daz_pkt()\n",
    "    while not len(offen) == 0:\n",
    "        datum = random.choice(offen)\n",
    "        vergabe_erfolgreich = False\n",
    "        while not len(arzt_nach_prio) == 0:\n",
    "            name = arzt_nach_prio.pop()\n",
    "            if vergabe_pruefen(name, datum, reihe):\n",
    "                dienst_vergeben(name, datum, reihe)\n",
    "                vergabe_erfolgreich = True\n",
    "                break\n",
    "        if not vergabe_erfolgreich:\n",
    "            unbesetzt_eintragen(datum, reihe)\n",
    "        offen = liste_rest_offen(reihe)\n",
    "        arzt_nach_prio = prio_arzt_daz_pkt()\n",
    "\n",
    "def show_dienstplan(version=0):\n",
    "    m = alle_versionen[version][1]\n",
    "    print(\"\\033[1m\"+\"NFA-Dienste:\"+\"\\033[0m\")\n",
    "    for tag in m:\n",
    "        if m[tag].get(\"frei\"):\n",
    "            print(\"\\033[1m\"+str(m[tag].get(\"nfa\"))+\"\\033[0m\")\n",
    "        else:\n",
    "            print(m[tag].get(\"nfa\"))\n",
    "\n",
    "    print(\" \")\n",
    "    print(\"\\033[1m\"+\"Haus-Dienste:\"+\"\\033[0m\")\n",
    "    for tag in m:\n",
    "        if m[tag].get(\"frei\"):\n",
    "            print(\"\\033[1m\"+str(m[tag].get(\"haus\"))+\"\\033[0m\")\n",
    "        else:\n",
    "            print(m[tag].get(\"haus\"))\n",
    "\n",
    "def show_d_pkt_we_dl(version=0):\n",
    "    a = alle_versionen[version][2]\n",
    "    m = alle_versionen[version][1]\n",
    "    print(\"Name\"+\" \"*8+\" | \"+\" D \"+\" | \"+\"Pkt\"+\" | \"+\"WEs\"+\" | \"+\"Dienste\")\n",
    "    print(\"-\"*60)\n",
    "    for arzt in a:\n",
    "        name = arzt\n",
    "        a1 = \".\"*(12-len(name))\n",
    "        pkt = str(float(a[arzt].get(\"punkte\")))\n",
    "        d = str(float(a[arzt].get(\"dienstanzahl\")))\n",
    "        we = str(float(a[arzt].get(\"wochenenden\")))\n",
    "        dl = \" \"\n",
    "        for ds in a[arzt].get(\"dienste\"):\n",
    "            if m[ds].get(\"punkte\") > 1:\n",
    "                dl = dl + \"\\033[1m\"+str(ds)+\", \\033[0m\"\n",
    "            else:\n",
    "                dl = dl + str(ds)+\", \"\n",
    "        print(name+a1+\" | \"+d+\" | \"+pkt+\" | \"+we+\" | \"+dl)\n",
    "\n",
    "\n",
    "def wochenenden_bewerten():\n",
    "    n = 0\n",
    "    for arzt in aerzte:\n",
    "        n += (aerzte[arzt].get(\"wochenenden\") * aerzte[arzt].get(\"wochenenden\"))\n",
    "    return n\n",
    "\n",
    "\n",
    "def dienstverteilung_bewerten():\n",
    "    summe = 0\n",
    "    bewertungsfaktor = 4\n",
    "    for arzt in aerzte:\n",
    "        summe += aerzte[arzt].get(\"dienstanzahl\")\n",
    "    mw = summe / len(aerzte)\n",
    "    abweichungssumme = 0\n",
    "    for arzt in aerzte:\n",
    "        abweichungssumme += ((aerzte[arzt].get(\"dienstanzahl\")-mw) * (aerzte[arzt].get(\"dienstanzahl\")-mw))\n",
    "    bewertung = abweichungssumme * bewertungsfaktor\n",
    "    return bewertung\n",
    "\n",
    "\n",
    "def punkteverteilung_bewerten():\n",
    "    summe = 0\n",
    "    bewertungsfaktor = 1.5\n",
    "    for arzt in aerzte:\n",
    "        summe += aerzte[arzt].get(\"punkte\")\n",
    "    mw = summe / len(aerzte)\n",
    "    abweichungssumme = 0\n",
    "    for arzt in aerzte:\n",
    "        abweichungssumme += ((aerzte[arzt].get(\"punkte\")-mw) * (aerzte[arzt].get(\"punkte\")-mw))\n",
    "    bewertung = abweichungssumme * bewertungsfaktor\n",
    "    return bewertung\n",
    "\n",
    "\n",
    "def unbesetzt_bewerten():\n",
    "    bewertung = 0\n",
    "    for tag in monat:\n",
    "        if \"unbesetzt\" in monat[tag].get(\"nfa\") or \"unbesetzt\" in monat[tag].get(\"haus\"):\n",
    "            bewertung = 500\n",
    "    return bewertung\n",
    "\n",
    "        \n",
    "def dienste_sortieren():\n",
    "    for arzt in aerzte:\n",
    "        aerzte[arzt][\"dienste\"] = sorted(aerzte[arzt].get(\"dienste\"))\n",
    "\n",
    "def folge_wochenenden_bewerten():\n",
    "    bewertung = 0\n",
    "    bewertungsfaktor = 2\n",
    "    we_tage = []\n",
    "    for tag in monat:\n",
    "        if monat[tag].get(\"punkte\") > 1:\n",
    "            we_tage.append(tag)\n",
    "    for arzt in aerzte:\n",
    "        we_dienste = []\n",
    "        for d in aerzte[arzt].get(\"dienste\"):\n",
    "            if d in we_tage:\n",
    "                we_dienste.append(d)\n",
    "        we_dienstabstaende = []\n",
    "        \n",
    "        if len(we_dienste) > 1:\n",
    "            for d in range(len(we_dienste)-1):\n",
    "                abstand = we_dienste[d+1] - we_dienste[d]\n",
    "                we_dienstabstaende.append(abstand)\n",
    "        pkt = 0\n",
    "        for abstand in range(len(we_dienstabstaende)):\n",
    "            if we_dienstabstaende[abstand] < 10:\n",
    "                pkt += 1\n",
    "                try:\n",
    "                    if we_dienstabstaende[abstand] + we_dienstabstaende[abstand+1] < 20:\n",
    "                        pkt += 5\n",
    "                except:\n",
    "                    pass\n",
    "        bewertung += pkt\n",
    "    bewertung = bewertung * bewertungsfaktor\n",
    "    return bewertung\n",
    "\n",
    "\n",
    "def version_bewerten_und_speichern():\n",
    "    global monat\n",
    "    global aerzte\n",
    "    dienste_sortieren()\n",
    "    wochenenden_pkt = wochenenden_bewerten()\n",
    "    dienstverteilung_pkt = dienstverteilung_bewerten()\n",
    "    punkteverteilung_pkt = punkteverteilung_bewerten()\n",
    "    unbesetzt_pkt = unbesetzt_bewerten()\n",
    "    folge_wochendenden_pkt = folge_wochenenden_bewerten()\n",
    "    summe_pkt = wochenenden_pkt + dienstverteilung_pkt + punkteverteilung_pkt + unbesetzt_pkt + folge_wochendenden_pkt\n",
